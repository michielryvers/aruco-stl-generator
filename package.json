{
  "name": "aruco-stl-generator",
  "version": "1.0.0",
  "description": "Generate 3D printable ArUco markers",
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./dist/styles.css --minify",
    "dev": "tailwindcss -i ./src/input.css -o ./dist/styles.css --watch",
    "serve": "python -m http.server 8000 || python3 -m http.server 8000",
    "start": "npm run serve"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  }
}
